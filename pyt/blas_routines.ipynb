   "source": [
    "# Matrix vector product\n",
    "\n",
    "A = np.random.randint(0,10,size=(4,4))\n",
    "x = np.random.randint(0,10,size=(4,1))\n",
    "b = np.dot(A,x)\n",
    "\n",
    "print(\"    {0}      {1} = {2}\".format('A','x','b'))\n",
    "\n",
    "for i in range(A.shape[1]):\n",
    "    print(A[i,:],x[i],b[i])"
   ]
